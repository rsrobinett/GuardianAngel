<header>
  <div class="navbar-fixed">
    <nav>
      <div class="nav-wrapper container">
        <a href="/location" class="brand-logo"><i class="material-icons">cloud</i>Guardian Angel</a>
        <ul id="nav-mobile" class="right">
          <li><a href="/location"><i class="material-icons right">person_pin</i>Location</a></li>
          <li><a href="/history"><i class="material-icons right">history</i>History</a></li>
          <li><a href="/logout"><i class="material-icons right">input</i>Logout</a></li>
        </ul>
      </div>
    </nav>
  </div>
</header>
<main>
  <div class="container">
    <div class="row">
      <div class='valign-wrapper'>
        <div class="col s10">
          <h3 class='map-header'>History</h3>
        </div>
        <div class="col s2 right-align">
          <a class="waves-effect waves-light btn valign">Download</a>
        </div>
      </div>
    </div>
    <table class='highlight'>
      <thead>
      <tr>
        <th>Date</th>
        <th>Longitude</th>
        <th>Latitude</th>
        <th>HeartRate</th>
        <th>View Map</th>
      </tr>
      </thead>
      <tbody>
      {{#each history}}
        <tr>
          <td>{{this.datatime}}</td>
          <td>{{this.longitude}}</td>
          <td>{{this.latitude}}</td>
          <td>{{this.heartrate}}</td>
          <td><a>map</a></td>
        </tr>
      {{/each}}
      </tbody>
    </table>
  </div>
</main>


